<div id="${parameters.name}" class="window">
        <h3 style="${parameters.cursor}" title="${parameters.tooltip}">${parameters.icon} <span class="title">${parameters.title}</span>
            <% if (Boolean.parseBoolean(parameters.editing)) { %><button class="close" type="button">x</button> <% } %>
        </h3>
        <div class="content">${parameters.content}</div>
    </div>